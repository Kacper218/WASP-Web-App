@model IEnumerable<WASP_Web_App.Entities.Keys>

@{
    ViewData["Title"] = "Index";
}

<h1>Keys</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Key_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Room)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.Key_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Room)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Key_ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Key_ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Key_ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
